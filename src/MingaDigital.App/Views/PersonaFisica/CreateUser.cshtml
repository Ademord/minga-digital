@model UsuarioEditorModel

@{
    ViewBag.Title = "Crear Usuario";
    
    var cancelUrl = Url.Action("Detail", new { id = Model.PersonaFisicaId });
}

<div class="row">
  <div class="col-md-offset-3 col-md-6">
    <h2 class="page-header">
      @ViewBag.Title
    </h2>
    
    @Component.Invoke("BasicEditor", Html, Model, cancelUrl)
  </div>
</div>